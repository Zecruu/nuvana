<svg width="800" height="400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="grad1" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1a1a2e;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#16213e;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0f3460;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="grad2" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#00d4ff;stop-opacity:0.8" />
      <stop offset="100%" style="stop-color:#5200ff;stop-opacity:0.8" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="400" fill="url(#grad1)"/>
  
  <!-- Search bar mockup -->
  <rect x="200" y="100" width="400" height="50" rx="25" fill="#ffffff" opacity="0.1" stroke="url(#grad2)" stroke-width="2"/>
  <circle cx="560" cy="125" r="15" fill="url(#grad2)" opacity="0.8"/>
  
  <!-- Search results -->
  <rect x="200" y="180" width="400" height="40" rx="5" fill="url(#grad2)" opacity="0.3"/>
  <rect x="200" y="240" width="400" height="40" rx="5" fill="#ffffff" opacity="0.1"/>
  <rect x="200" y="300" width="400" height="40" rx="5" fill="#ffffff" opacity="0.1"/>
  
  <!-- Location pin -->
  <circle cx="150" cy="200" r="30" fill="url(#grad2)" opacity="0.4"/>
  <path d="M 150 180 L 150 220 M 140 200 L 160 200" stroke="url(#grad2)" stroke-width="4"/>
  
  <!-- Map grid -->
  <g opacity="0.3">
    <line x1="650" y1="150" x2="750" y2="150" stroke="url(#grad2)" stroke-width="1"/>
    <line x1="650" y1="180" x2="750" y2="180" stroke="url(#grad2)" stroke-width="1"/>
    <line x1="650" y1="210" x2="750" y2="210" stroke="url(#grad2)" stroke-width="1"/>
    <line x1="670" y1="130" x2="670" y2="230" stroke="url(#grad2)" stroke-width="1"/>
    <line x1="700" y1="130" x2="700" y2="230" stroke="url(#grad2)" stroke-width="1"/>
    <line x1="730" y1="130" x2="730" y2="230" stroke="url(#grad2)" stroke-width="1"/>
  </g>
  
  <!-- Title -->
  <text x="400" y="50" font-family="Arial, sans-serif" font-size="24" font-weight="bold" text-anchor="middle" fill="#00d4ff">Local SEO</text>
  <text x="400" y="380" font-family="Arial, sans-serif" font-size="18" text-anchor="middle" fill="#ffffff" opacity="0.8">Puerto Rico 2025</text>
</svg>
